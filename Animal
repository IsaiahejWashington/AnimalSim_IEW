import java.util.Random;

public abstract class Animal {
	
	protected int simID;
	protected int Location;
	protected boolean full;
	protected boolean rested;
	
	
	
	public Animal() {
		simID = 0;
		Location = [0,0];//why is there an error message here?
		full = false;
		rested = true 
		
	}
	
	public Animal(int simID, Location I) {
		this.simID = simID;
		this.Location = Location;
		this.full = false;
		this.rested = true;
		
	}
	
	public boolean eat() {
		Random r = new Random();
		
		if (r<.5) {
			return full = false;
		}
		(r>.5)
		return full = true;
		
		
	}
	
	public boolean sleep() {
		Random r = new Random();
		
		if(r<.5) {
			return rested = false;
			}
		(r>.5)
		return rested = true;
		
	}

	public int getSimID() {
		return simID;
	}

	public void setSimID(int simID) {
		this.simID = simID;
	}

	public int getLocation() {
		return Location;
	}

	public void setLocation(int location) {
		Location = location;
	}

	public boolean isFull() {
		return full;
	}

	public void setFull(boolean full) {
		this.full = full;
	}

	public boolean isRested() {
		return rested;
	}

	public void setRested(boolean rested) {
		this.rested = rested;
	}

	
}
